spring.thymeleaf.cache: false

logging:
  file: ${user.home}/logs/web.log

web:
 ips: /*
 tokenHeader:
 fileUploadPath: E:\uploadImage

server.port: 8082

spring.http.multipart:
  #默认支持文件上传.
  enabled: true
  #支持文件写入磁盘.
  file-size-threshold: 0
  # 上传文件的临时目录
  location:
  # 最大支持文件大小
  max-file-size: 500Mb
  # 最大支持请求大小
  max-request-size: 100Mb
